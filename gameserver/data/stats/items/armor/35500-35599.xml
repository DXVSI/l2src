<list>
	<armor id="35564" name="Rulers Authority">
		<!-- gorodetskiy -->
		<!-- Предмет, выдаваемый победителю Фестиваля Хаоса в Таинственном Особняке. 
		При ношении Макс. HP/MP/CP +500, лимит веса +12000, 
		Скор. Восстановления HP +10.12, Скор. Восстановления MP +3,89, Физ. Атк./ Маг. Атк./ Физ. Защ./ Маг. Защ. +6%, 
		сопротивление отражению урона +5, +36 ячеек инвентаря, защита от всех стихий +10. -->
        <set name="type" value="NONE"/>
		<set name="icon" value="icon.belt_rulers_authority"/>
        <set name="weight" value="440"/>
        <equip>
            <slot id="BELT"/> 
        </equip>
        <set name="ex_type" value="BELT"/>
        <for>
            <add stat="maxHp" order="0x40" value="500"/>
			<add stat="maxMp" order="0x40" value="500"/>
			<add stat="maxCp" order="0x40" value="500"/>
			<add stat="maxLoad" order="0x40" value="12000"/>
			<add stat="regHp" order="0x40" value="10.12"/>
			<add stat="regMp" order="0x40" value="3.89"/>
			<add stat="inventoryLimit" order="0x40" value="36"/>
			<mul stat="pAtk" order="0x30" value="1.06"/>
			<mul stat="mAtk" order="0x30" value="1.06"/>
			<mul stat="pDef" order="0x30" value="1.06"/>
			<mul stat="mDef" order="0x30" value="1.06"/>
			<add stat="defenceFire" order="0x40" value="10"/>
			<add stat="defenceWater" order="0x40" value="10"/>
			<add stat="defenceWind" order="0x40" value="10"/>
			<add stat="defenceEarth" order="0x40" value="10"/>
			<add stat="defenceHoly" order="0x40" value="10"/>
			<add stat="defenceUnholy" order="0x40" value="10"/>
			<enchant stat="mDef" order="0x0C" value="0"/>
			<!-- сопротивление отражению урона. Дописать -->
        </for>
    </armor>
	<armor id="35565" name="Mysterious Belt">
		<!-- gorodetskiy -->
		<!-- Предмет, выдаваемый победителю Фестиваля Хаоса в Таинственном Особняке. 
		При ношении Макс. HP/MP/CP +500, Скор. Восстановления HP +7.1, Скор. Восстановления MP +2.74, наносимый урон в PvP +5%,
		сопротивление отражению урона +5, получаемый урон в PvP -6%. Нельзя обменять/выбросить/продать через личную торговую лавку. -->
		<set name="dropable" value="false"/>
		<set name="tradeable" value="false"/>
        <set name="augmentable" value="false"/>
        <set name="type" value="NONE"/>
		<set name="icon" value="icon.belt_rulers_authority"/>
        <set name="weight" value="440"/>
		<set name="durability" value="259200"/>
        <equip>
            <slot id="BELT"/> 
        </equip>
        <set name="ex_type" value="BELT"/>
        <for>
            <add stat="maxHp" order="0x40" value="500"/>
			<add stat="maxMp" order="0x40" value="500"/>
			<add stat="maxCp" order="0x40" value="500"/>
			<add stat="regHp" order="0x40" value="7.1"/>
			<add stat="regMp" order="0x40" value="2.74"/>
			<add stat="regMp" order="0x40" value="2.74"/>
			<mul stat="pvpPhysDefenceBonus" order="0x30" value="1.06"/> <!-- получаемый урон в PvP -6% -->
			<mul stat="pvpPhysSkillDefenceBonus" order="0x30" value="1.06"/> <!-- получаемый урон в PvP -6% -->
			<mul stat="pvpMagicSkillDefenceBonus" order="0x30" value="1.06"/> <!-- получаемый урон в PvP -6% -->
			<mul stat="pvpMagicSkillDmgBonus" order="0x30" value="1.05"/> <!-- наносимый урон в PvP +5% -->
			<mul stat="pvpPhysSkillDmgBonus" order="0x30" value="1.05"/> <!-- наносимый урон в PvP +5% -->
			<mul stat="pvpPhysDmgBonus" order="0x30" value="1.05"/> <!-- наносимый урон в PvP +5% -->
			<enchant stat="mDef" order="0x0C" value="0"/>
			<!--  сопротивление отражению урона +5. Дописать -->
        </for>
    </armor>
