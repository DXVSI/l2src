	<skill id="13313" levels="1" name="Благословенная Серьга Антараса">
		<!--
			Увеличивает Защиту/Маг. Защиту; защиту от земли/света; сопротивление кровопусканию/шоковым/ментальным атакам; 
			добавляет бонус кровопусканию/шоковым/ментальным атакам; усиливает эффективность магии, восстанавливающей HP;
			уменьшает расход MP на умения; 
			с определенной вероятностью восстанавливает HP за счет урона, нанесенного врагу. ВЫН +3, ДУХ +3.	
		-->
		<set name="icon" val="icon.accessory_necklace_of_valakas_i00" />
		<set name="magicLevel" val="90" />
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<add order="0x40" stat="bleedResist" val="50" />
			<add order="0x40" stat="bleedPower" val="50" />
			<mul order="0x30" stat="hpEff" val="1.40" />
			<add order="0x40" stat="mentalResist" val="40" />
			<add order="0x40" stat="mentalPower" val="40" />
			<add order="0x40" stat="stunResist" val="40" />
			<add order="0x40" stat="stunPower" val="40" />			
			<add order="0x40" stat="absorbDam" val="4" />
			<mul order="0x30" stat="mpConsum" val="0.90" />			
			<add order="0x40" stat="defenceEarth" val="30" />
			<add order="0x40" stat="defenceHoly" val="15" />			
			<add order="0x10" stat="CON" val="3"/>			
			<add order="0x10" stat="MEN" val="3"/>			
		</for>
	</skill>
	
	<skill id="13315" levels="1" name="Благословенное Ожерелье Валакаса">
		<!--
			Увеличивает сопротивление атакам Огня/Тьмы, усыпляющим атакам; 
			добавляет бонус усыпляющим атакам; 
			увеличивает Макс. HP/Физ. Атк./Маг. Атк/ шанс Маг. Крит. Атк.; 
			сокращает время перезарядки умения; отражает часть полученного урона обратно врагу. 
			СИЛ+3, ИНТ +3.		
		-->
		<set name="icon" val="icon.accessory_necklace_of_valakas_i00" />
		<set name="magicLevel" val="90" />
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<add order="0x60" stat="maxHp" val="625" />
			<add order="0x40" stat="sleepResist" val="50" />
			<add order="0x40" stat="sleepPower" val="50" />
			<mul order="0x30" stat="mReuse" val="0.90" />
			<mul order="0x30" stat="pReuse" val="0.90" />			
			<mul order="0x30" stat="pAtk" val="1.10" />
			<mul order="0x30" stat="mAtk" val="1.20" />
			<add order="0x40" stat="mCritRate" val="1" />
			<add order="0x40" stat="reflectDam" val="5" />
			<add order="0x40" stat="defenceFire" val="30" />
			<add order="0x40" stat="defenceUnholy" val="15" />	
			<add order="0x10" stat="STR" val="3"/>			
			<add order="0x10" stat="INT" val="3"/>			
		</for>
	</skill>
	<skill id="13317" levels="1" name="Кольцо Земляного Червя">
		<!--gorodetskiy
			MP +38, увеличивает сопротивление Физ. Атк./Мент. Атк., регенерацию MP, силу Крит. Атк., Точность. 
			Сопротивление отравлению +50%, шанс отравить +50%, шанс опрокидывания, отталкивания, сопротивление притягиванию +20. 
			Увеличивает Скор. Атк./Маг., сопротивление ветру/воде. Увеличивает Скор. Атк./Маг. членов группы и дарует им Ярость Вампира. 
			При надевании двух таких колец эффект не суммируется.	
		-->
		<set name="icon" val="icon.accessory_necklace_of_valakas_i00" />
		<set name="magicLevel" val="90" />
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<add order="0x60" stat="maxMp" val="38" />
			<mul order="0x30" stat="pDef" val="1.05" /><!-- с потолка -->
			<mul order="0x30" stat="mentalResist" val="1.05" /><!-- с потолка -->
			<mul order="0x30" stat="regMp" val="1.03" /><!-- с потолка -->
			<mul order="0x30" stat="rCrit" val="1.03" /><!-- с потолка -->
			<mul order="0x30" stat="accCombat" val="1.05" /><!-- с потолка -->
			<add order="0x40" stat="poisonResist" val="50" />
			<add order="0x40" stat="poisonPower" val="50" />
			<mul order="0x30" stat="mReuse" val="0.90" />
			<mul order="0x30" stat="pReuse" val="0.90" />			
			<mul order="0x30" stat="pAtk" val="1.10" />
			<mul order="0x30" stat="mAtk" val="1.20" />
			<add order="0x40" stat="mCritRate" val="1" />
			<add order="0x40" stat="reflectDam" val="5" />
			<add order="0x40" stat="defenceFire" val="30" />
			<add order="0x40" stat="defenceUnholy" val="15" />	
		</for>
	</skill>
